{
  "name": "lezhnev74/pasvl",
  "description": "Array Validator (regular expressions for nested array, sort of)",
  "keywords": [
    "validation",
    "validator",
    "array",
    "structure",
    "pattern"
  ],
  "authors": [
    {
      "name": "Dmitriy Lezhnev",
      "email": "lezhnev.work@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0|^8.0|^9.0",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "PASVL\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PASVL\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "init-git-hook": [
      "ln -s -f ../../.githooks/pre-commit .git/hooks/pre-commit",
      "chmod +x .git/hooks/pre-commit"
    ]
  }
}
